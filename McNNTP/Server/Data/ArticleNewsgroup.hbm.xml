<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="McNNTP" namespace="McNNTP.Server.Data">
  <class name="ArticleNewsgroup" table="ArticleNewsgroup" lazy="true">
    <id name="Id" type="Int32">
      <generator class="native" />
    </id>
    <many-to-one name="Newsgroup" column="NewsgroupId" not-null="true" lazy="false" />
    <many-to-one name="Article" column="ArticleId" not-null="true" lazy="false" />
    <property name="Number" not-null="true" update="false" />
    <property name="Cancelled" not-null="true" />
    <property name="Pending" not-null="true" />
  </class>
</hibernate-mapping>