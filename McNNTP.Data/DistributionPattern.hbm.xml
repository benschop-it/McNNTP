<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="McNNTP.Data" namespace="McNNTP.Data">
  <class name="DistributionPattern" table="DistributionPattern" lazy="true">
    <id name="Id" type="Int32">
      <generator class="native" />
    </id>
    
    <property name="Weight" not-null="true" length="1000" />
    <property name="Wildmat" not-null="true" length="1000" unique="true" />
    <property name="Distribution" not-null="true" length="1000" />
  </class>
</hibernate-mapping>